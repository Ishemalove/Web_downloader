<script>
var session_setter = function () {
    var xhr = new XMLHttpRequest();
    xhr.withCredentials = true;
    xhr.open("GET", 'https://www.codecademy.com/users/session?skip_initial_referrer=true');
    xhr.setRequestHeader("Access-Control-Allow-Credentials","true");
    xhr.setRequestHeader('Content-Type', 'application/json');
    xhr.send();
}
setTimeout(session_setter,0);


var user_visit = function () {
    var xhr = new XMLHttpRequest();
        xhr.withCredentials = true;
        xhr.open("POST", 'https://www.codecademy.com/analytics/user' + window.location.search, true);
        xhr.setRequestHeader("Access-Control-Allow-Credentials","true");
        xhr.setRequestHeader('Content-Type', 'application/json');
        xhr.send(JSON.stringify({
        category: 'user',
        event: 'visit',
        properties: {"page_name": "instapage",
                    "source_codebase":"monolith",
                    "fullpath":window.location.pathname,
                    "search":window.location.search,
                    "path":window.location.pathname,
                    "title": document.title,
                    "url":window.location.href,
                    "referrer":document.referrer,
                    "client":"default",
                    "misc": JSON.stringify({ 
                        "instapage_variant": __variant,
                        "instapage_variant_name": __variant_custom_name,
                        "instapage_experiment_id": __experiment_id
                        })
                    },
        gdpr_safe: 'undefined'
    }));
}
setTimeout(user_visit,1900);

window.addEventListener('DOMContentLoaded', function () {
    function handler(e) {
      var link = e.target;
      var xhr = new XMLHttpRequest(); 
      xhr.withCredentials = true;
      xhr.open("POST", 'https://www.codecademy.com/analytics/user' + window.location.search, true);
      xhr.setRequestHeader("Access-Control-Allow-Credentials","true");
      xhr.setRequestHeader('Content-Type', 'application/json');
      xhr.send(JSON.stringify({
      category: 'user',
      event: 'click',
      properties: {"page_name": "instapage",
                "source_codebase":"monolith",
                "fullpath":window.location.pathname,
                "search":window.location.search,
                "path":window.location.pathname,
                "title": document.title,
                "url":window.location.href,
                "referrer":document.referrer,
                "client":"default",
                "target": link.href,
                "misc": JSON.stringify({ 
                    "instapage_variant": __variant,
                    "instapage_variant_name": __variant_custom_name,
                    "instapage_experiment_id": __experiment_id
                    })
                },
       gdpr_safe: 'undefined'
       }));
    }
  
    var elements = document.querySelectorAll("a");
    for (var i = 0; i < elements.length; i++) {
        var element = elements[i];
        if (element.parentElement.id && element.parentElement.id.match(/^element-/)) {
            element.addEventListener("click", handler);
        }
    }
});
</script>

